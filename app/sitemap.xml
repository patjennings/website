---
layout: null
search: exclude
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>/about</loc>
    <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>/contact</loc>
    <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>/references</loc>
    <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>/projets</loc>
    <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  {% for project in site.projects %}
  <url>
    <loc>{{project.url}}</loc>
    <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.5</priority>
  </url>
  {% endfor %}

</urlset>
